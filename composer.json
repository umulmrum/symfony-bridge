{
    "name": "bref/symfony-bridge",
    "description": "Package that makes Symfony work on AWS Lambda with Bref",
    "keywords": [
        "aws",
        "lambda",
        "symfony"
    ],
    "license": "MIT",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Bref\\SymfonyBridge\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bref\\SymfonyBridge\\Test\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/umulmrum/bref"
        }
    ],
    "require": {
        "php": ">=7.3",
        "bref/bref": "dev-remove-silly-dependency as 1.4.99",
        "symfony/filesystem": "^5.0|^6.0",
        "symfony/http-kernel": "^5.0|^6.0"
    },
    "require-dev": {
        "mnapoli/hard-mode": "^0.3.0",
        "phpunit/phpunit": "^8.5.14",
        "symfony/config": "^5.0|^6.0",
        "symfony/dependency-injection": "^5.0|^6.0",
        "symfony/process": "^5.0|^6.0"
    },
    "config": {
        "sort-packages": true
    }
}
